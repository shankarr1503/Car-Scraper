# Apify Configuration
APIFY_TOKEN=your_apify_token_here
APIFY_PROXY_PASSWORD=your_proxy_password_here

# Security Settings
MAX_REQUESTS_PER_MINUTE=60
REQUEST_TIMEOUT=30000
MAX_RETRIES=3

# Proxy Configuration
PROXY_TYPE=residential
PROXY_COUNTRY=US

# Rate Limiting
REQUEST_DELAY_MIN=1000
REQUEST_DELAY_MAX=5000

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true

# Database (if needed)
DATABASE_URL=postgresql://username:password@localhost:5432/car_scraper

# External APIs (if needed)
EXTERNAL_API_KEY=your_api_key_here

# Development
NODE_ENV=development
DEBUG=false